<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.PrimaryController">
    <children>
            <!-- Add buttons for each student in a separate row -->
            <Button fx:id="st1" text="John Doe" onAction="#st1show" />
            <Button fx:id="st2" text="Jane Smith" onAction="#st2show" />
            <Button fx:id="st3" text="Michael Johnson" onAction="#st3show" />
            <Button fx:id="st4" text="Emily Williams" onAction="#st4show" />
            <Button fx:id="st5" text="Robert Brown" onAction="#st5show" />
            <Button fx:id="st6" text="Olivia Davis" onAction="#st6show" />
            <Button fx:id="st7" text="William Miller" onAction="#st7show" />
            <Button fx:id="st8" text="Sophia Wilson" onAction="#st8show" />
            <Button fx:id="st9" text="James Taylor" onAction="#st9show" />
            <Button fx:id="st10" text="Elizabeth Anderson" onAction="#st10show" />

            <Button fx:id="showGradeBtn" text="Show Grade" onAction="#show" />
            <Button fx:id="changeGradeBtn" text="Change Grade" onAction="#changeGrade" />

        <TextField fx:id="messageTextField" promptText="Send a message" onAction="#changeGrade"/>
        <Label fx:id="resultLabel" />
    </children>
</VBox>
