<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ScanPage">
   <children>
      <ListView fx:id="exam_list" prefHeight="181.0" prefWidth="200.0" />
      <Label layoutX="328.0" layoutY="181.0" text="Correct Answer" textFill="#ff0606" />
      <Label layoutX="342.0" layoutY="92.0" text="Your Answer" textFill="#e80909" />
      <Button layoutX="534.0" layoutY="13.0" mnemonicParsing="false" onAction="#Back_but" text="Back" />
      <TextArea fx:id="Your_ans" layoutX="502.0" layoutY="78.0" prefHeight="38.0" prefWidth="41.0" />
      <TextArea fx:id="Corr_Ans" layoutX="502.0" layoutY="162.0" prefHeight="38.0" prefWidth="28.0" />
      <ListView fx:id="Questions_exam_list" layoutY="215.0" onMousePressed="#Questions_exam_list_press" prefHeight="192.0" prefWidth="200.0" />
      <Button layoutX="23.0" layoutY="186.0" mnemonicParsing="false" onAction="#Show_Ques" text="Show Questions" />
      <Label fx:id="wrong_ans" layoutX="283.0" layoutY="127.0" text="Wrong Answer!!" textFill="#e33107">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="done_but" layoutX="521.0" layoutY="357.0" mnemonicParsing="false" onAction="#Done_send_grade" text="Done" />
      <TextField fx:id="Ques_notes" layoutX="243.0" layoutY="228.0" prefHeight="76.0" prefWidth="266.0" />
      <TextArea fx:id="Std_notes" layoutX="200.0" layoutY="228.0" prefHeight="120.0" prefWidth="378.0" />
      <Button fx:id="insert_note_but" layoutX="302.0" layoutY="357.0" mnemonicParsing="false" onAction="#insert_note" text="Insert Note" />
   </children>
</AnchorPane>
